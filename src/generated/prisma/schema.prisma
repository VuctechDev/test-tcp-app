generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model records {
  id        Int      @id @default(autoincrement())
  deviceId  String
  long      Decimal
  lat       Decimal
  speed     Int
  createdAt DateTime @default(now())
}
